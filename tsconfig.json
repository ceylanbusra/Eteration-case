{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true
},
"typeRoots": ["./types/env.d.ts"],
"baseUrl": "./",
"paths": {
  "*": [
    "./src/*"
  ],
  "assets/*": [
    "./src/assets/*"
  ],
  "components/*": [
    "./src/components/*"
  ],
  "screens/*": [
    "./src/screens/*"
  ],
  "navigation/*": [
    "src/navigation/*"
  ],
  "store/*": [
    "./src/store/*"
  ],

},
"include": [
  "src",
  "App.tsx",
  ".eslintrc.js",
  "node_modules/@types/jest/index.d.ts"
]
}
